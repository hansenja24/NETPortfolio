@model List<Portfolio.Models.Project>

@{
    ViewData["Title"] = "Project";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Top Three Project: </h1>
    <div class="card mb-3">
        @for (var i = 0; i < Model.Count(); i++)
        {       
            <div class="card-block">
                <h1 class="card-title">@Model[i].Name</h1>
                <h2>@Model[i].Language</h2>
                <a href="@Model[i].Html_Url">@Model[i].Html_Url</a>
                <hr />
            </div>
        }
       <h4>Check More on: </h4> <a href="https://github.com/hansenja24">Github: Hansenja24</a>
    </div>